{
  "name": "ai-extraction-tester",
  "version": "0.1.0",
  "description": "Automated API-triggerable testing framework for AI extraction services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "cli": "ts-node src/cli/runner.ts",
    "api": "ts-node src/api/server.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "testing",
    "ai",
    "extraction",
    "json-diff",
    "regression-testing",
    "quality-benchmarking"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "deep-diff": "^1.0.2",
    "express": "^4.18.2",
    "yaml": "^2.3.4",
    "zod": "^3.22.4",
    "markdown-it": "^14.0.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/deep-diff": "^1.0.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2"
  }
}
